      "text/latex": [
       "\\begin{minted}{r}\n",
       "function (X, ncp = 5, ind.sup = NULL, quanti.sup = NULL, quali.sup = NULL, \n",
       "    excl = NULL, graph = TRUE, level.ventil = 0, axes = c(1, \n",
